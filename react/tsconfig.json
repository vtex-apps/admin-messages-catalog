{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "jsx": "preserve",
    "lib": [ "es2017", "dom" ],
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "target": "es2017",
    "skipLibCheck": true,
    "strict": true
  },
  "typeAcquisition": {
    "enable": false
  },
  "include": [
    "./**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
